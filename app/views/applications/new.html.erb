<h1>New Application</h1>
<%= form_with url: "/applications/:id", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Save" %>
<% end %>
